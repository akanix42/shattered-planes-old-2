{
  "compilerOptions": {
    "alwaysStrict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "ES2019",
    "baseUrl": "src",
    "paths": {
      "config": ["../config"]
    }
  }
}
